{
    "name": "antriver/laravel-multi-cache",
    "description": "Combine multiple caches in Laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Anthony Kuske",
            "homepage": "https://anthonykuske.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Antriver\\LaravelMultiCache\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Antriver\\LaravelMultiCacheTests\\": "tests/"
        }
    },
    "require": {
        "php": ">=8.1",
        "laravel/framework": "^10.0 || ^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5",
        "mockery/mockery": "^1.6",
        "orchestra/testbench": "^8.19"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Antriver\\LaravelMultiCache\\MultiStoreServiceProvider"
            ]
        }
    }
}
